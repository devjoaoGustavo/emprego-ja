<span class="response"><%= flash[:error] %></span>
<div class="form">
  <%= form_for(@company) do |f| %>
    <p><%= f.label :name %><br>
    <%= f.text_field :name %></p>
    <p><%= f.label :location %><br>
    <%= f.text_field :location %></p>
    <p><%= f.label :email %><br>
    <%= f.text_field :email %></p>
    <p><%= f.label :phone %><br>
    <%= f.text_field :phone %></p>
    <p><%= f.label :logo %><br>
    <%= f.file_field :logo %></p>
    <%= f.submit type %>
  <% end %>
</div>
<%= link_to 'Voltar', root_path %>
