<h2>Log-in</h2>
<hr>
<span class="response"><%= notice %></span>
<span class="error"><%= flash[:error] %></span>
<div class="login">
  <%= form_tag sessions_path do %>
    <p><%= label_tag :email %><br>
    <%= email_field_tag :email, params[:email] %></p>
    <p><%= label_tag :password %><br>
    <%= password_field_tag :password, nil %></p>
    <%= submit_tag 'Acessar' %>
  <% end %>
</div>
